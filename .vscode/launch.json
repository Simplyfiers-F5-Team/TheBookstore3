{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch via NPM",
			"request": "launch",
			"runtimeExecutable": "npm",
			"runtimeArgs": [
				"run-script",
				"start"
			],
			"envFile": "${workspaceFolder}/.env",
			"type": "node",
			"skipFiles": [
				"<node_internals>/**"
			],
		},
	]
}